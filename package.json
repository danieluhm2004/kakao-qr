{
  "name": "kakao-qr",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/danieluhm2004/kakao-qr.git",
  "author": "danieluhm2004 <iam@dan.al>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "start:prod": "cross-env NODE_ENV=production node .",
    "start:dev": "cross-env NODE_ENV=development node .",
    "start:stage": "cross-env NODE_ENV=stage node .",
    "serve": "yarn clean && cross-env NODE_ENV=development tsc-watch --onSuccess \"node .\"",
    "serve:prod": "yarn clean && cross-env NODE_ENV=production tsc-watch --onSuccess \"node .\"",
    "serve:stage": "yarn clean && cross-env NODE_ENV=staging tsc-watch --onSuccess \"node .\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn clean && tsc",
    "build:docker": "yarn clean && docker build ",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.8.0",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/crypto-js": "^4.0.1",
    "@types/eslint": "^7.2.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.9",
    "@types/prettier": "^2.1.5",
    "@types/tough-cookie": "^4.0.0",
    "@types/tough-cookie-file-store": "^2.0.0",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "tsc-watch": "4.1.0",
    "typescript": "^4.1.2"
  }
}
